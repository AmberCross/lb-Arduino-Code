                                 ;---------------------------------------------------------------------------------------------------------------------------
                                 ; Standard ATmega328 jump vectors
                                 
000000 940c 0034                  jmp RESET ; Reset Handler
000002 940c 0034                  jmp EXT_INT0 ; IRQ0 Handler
000004 940c 0034                  jmp EXT_INT1 ; IRQ1 Handler
000006 940c 0034                  jmp PC_INT0 ; PCINT0 Handler
000008 940c 0034                  jmp PC_INT1 ; PCINT1 Handler
00000a 940c 0034                  jmp PC_INT2 ; PCINT2 Handler
00000c 940c 0034                  jmp WDT ; Watchdog Timer Handler
00000e 940c 0034                  jmp TIM2_COMPA ; Timer2 Compare A Handler
000010 940c 0034                  jmp TIM2_COMPB ; Timer2 Compare B Handler
000012 940c 0034                  jmp TIM2_OVF ; Timer2 Overflow Handler
000014 940c 0034                  jmp TIM1_CAPT ; Timer1 Capture Handler
000016 940c 0101                  jmp TIM1_COMPA ; Timer1 Compare A Handler
000018 940c 0034                  jmp TIM1_COMPB ; Timer1 Compare B Handler
00001a 940c 0034                  jmp TIM1_OVF ; Timer1 Overflow Handler
00001c 940c 0034                  jmp TIM0_COMPA ; Timer0 Compare A Handler
00001e 940c 0034                  jmp TIM0_COMPB ; Timer0 Compare B Handler
000020 940c 0034                  jmp TIM0_OVF ; Timer0 Overflow Handler
000022 940c 0034                  jmp SPI_STC ; SPI Transfer Complete Handler
000024 940c 0034                  jmp USART_RXC ; USART, RX Complete Handler
000026 940c 0034                  jmp USART_UDRE ; USART, UDR Empty Handler
000028 940c 0034                  jmp USART_TXC ; USART, TX Complete Handler
00002a 940c 0034                  jmp ADCC ; ADC Conversion Complete Handler
00002c 940c 0034                  jmp EE_RDY ; EEPROM Ready Handler
00002e 940c 0034                  jmp ANA_COMP ; Analog Comparator Handler
000030 940c 0034                  jmp TWI ; 2-wire Serial Interface Handler
000032 940c 0034                  jmp SPM_RDY ; Store Program Memory Ready Handler
                                 	
                                 ;---------------------------------------------------------------------------------------------------------------------------
                                 ; unused vectors point to software reset code
                                  EXT_INT0:
                                  EXT_INT1:
                                  PC_INT0:
                                  PC_INT1:
                                  PC_INT2:
                                  WDT:
                                  TIM2_COMPA:
                                  TIM2_COMPB:
                                  TIM2_OVF:
                                  TIM1_CAPT:
                                  ;TIM1_COMPA: //used elsewhere
                                  TIM1_COMPB:
                                  TIM1_OVF:
                                  TIM0_COMPA:
                                  TIM0_COMPB:
                                  TIM0_OVF:
                                  SPI_STC:
                                  USART_RXC:
                                  USART_UDRE:
                                  USART_TXC:
                                  ADCC:
                                  EE_RDY:
                                  ANA_COMP:
                                  TWI:
                                  SPM_RDY:
                                 
                                 ;---------------------------------------------------------------------------------------------------------------------------
                                 ; Program start
                                 
                                 RESET:							
000034 94f8                      	CLI							; disable interupts
                                 
                                 ; Initialize the I/O Ports
000035 ef0f                      	LDI		J, 0xFF				
000036 b905                      	out		PORTB, J			; enable pullups
000037 b908                      	out		PORTC, J			; enable pullups
000038 b90b                      	out		PORTD, J			; enable pullups
                                 
000039 e000                      	LDI		J, 0x00				; pins 0 - 7 of port D
00003a b90a                      	out		DDRD, J				; set PORTD pins to inputs TEMPORARILY to read config bits
                                 
00003b e001                      	LDI		J, 0x01				; pin 1 of port B is output (sync)
00003c b904                      	out		DDRB, J			
                                 
                                 ; Initialize Timer 1 
                                 ;   OCR1A will hold the value of 1016/1024 and be used to reset the counter and 
                                 ;   cause an IRQ that will start the HSYNC process
                                 ;   The counter will count each clock cycle (16MHz - no prescaling)
                                 
00003d e000                      	ldi     J, 0x00
00003e 9300 0080                 	sts     TCCR1A, J
000040 e009                      	ldi     J, 0x09				; set timer 1 to CTC mode 4
000041 9300 0081                 	sts     TCCR1B, J		
                                 
000043 e002                      	ldi     J, 0x02				; allow OCR1A IRQ's
000044 9300 006f                 	sts     TIMSK1, J			
                                 
                                 ; Initialise other registers
000046 e000                      	ldi		J,0x00
000047 9300 00c1                 	sts		UCSR0B,J			; Turn off serial
                                 
000049 e800                      	ldi     J, 0x80
00004a bf00                      	out     ACSR, J				; turn off comparator
                                 
00004b e008                      	ldi		J, 0x08				; set stack pointer to top of SRAM 
00004c bf0e                      	out		SPH, J				;
00004d ef0f                      	ldi		J, 0xFF				;
00004e bf0d                      	out		SPL, J				;
                                 
                                 ; Initialise Program Variables
00004f e060                      	ldi		chrln, 0x00			; keeps track of font gen line (0-7)
000050 e050                      	ldi		vlineh, 0x00		; vertical line counter 0-262
000051 e040                      	ldi		vline, 0x00			;			"
000052 e0c0                      	ldi		YL, 0x00			;
000053 e0d1                      	ldi		YH, 0x01			; init y pointer
000054 e020                      	ldi		column, 0x00		; keeps track of horizontal chr pos (0-79)
000055 e030                      	ldi		row, 0x00			; keeps track of vertical chr pos (0-24)
000056 e090                      	ldi		cursorClk, 0x00		; init cursor blink timer
000057 8078                      	ld		charUnderCursor, Y	; init cursor character
000058 e58f                      	ldi		cursorChar, 0x5F	; init cursor '_' character
000059 ef0f                      	ldi		J,	0xFF	
00005a 2e60                      	mov		previousInpt, J
00005b e000                      	ldi		J,	0x00
00005c 2ec0                      	mov		cmdVal, J
                                 
                                 ;---------------------------------------------------------------------------------------------------------------------------
                                 ; Set configuration from PORTD
00005d b039                      	in		configByte,PIND		; Read configuration bits (PORTD pins)
                                 
                                 								; Configuration now read, so set PORTD pins to outputs
00005e e000                      	ldi		J, 0x00				; 
00005f b90b                      	out		PORTD, J			; set port D outputs to 0
000060 ef0f                      	LDI		J, 0xFF				; pins 0 - 7 of port B
000061 b90a                      	out		DDRD, J				; set PORTD pins to outputs
                                 
                                 ; HARD CODED OVERRIDE IF PREFERRED
                                 ;	ldi		J, 1<<CONFIG_PAL | 0<<CONFIG_8_OR_4_BIT | 1<<CONFIG_N_TWI | 1<<CONFIG_80_CHAR_PER_LINE | 1<<CONFIG_SINGLE_HEIGHT | 0<<CONFIG_BOLD
                                 ;	mov		configByte,J
                                 
                                 ;---------------------------------------------------------------------------------------------------------------------------
                                 ; Set default font
000062 e000                      	ldi		J, 0x00
000063 fc34                      	sbrc	configByte,CONFIG_80_CHAR_PER_LINE ; 80 if high
000064 6001                      	ori		J, 1<<ATT_80_CHAR_PER_LINE
000065 fc32                      	sbrc	configByte,CONFIG_BOLD ; Bold if high
000066 6002                      	ori		J, 1<<ATT_BOLD
000067 fe33                      	sbrs	configByte,CONFIG_SINGLE_HEIGHT ; Double if low
000068 6004                      	ori		J, 1<<ATT_DBL_HEIGHT
000069 2e50                      	mov		activeLineAtt, J
                                 
                                 ;---------------------------------------------------------------------------------------------------------------------------
                                 ; Select PAL/NTSC System
00006a fe37                      	sbrs	configByte, CONFIG_PAL	; low = NTSC
00006b c00b                      	rjmp	setNTSC				; Yes, set NTSC mode
                                 
                                 setPAL:
00006c e308                      	ldi		J, 0x38				; 0x0138 Last vertical line 312
00006d 2ea0                      	mov		lastline, J			;
00006e e400                      	ldi		J, 0x40				; first vertical line of active disp
00006f 2eb0                      	mov		line1, J			;
000070 e004                      	ldi		J, 0x04				;  0x0400 = 1024 clocks per line		
000071 9300 0089                 	sts     OCR1AH, J        
000073 e000                      	ldi		J, 0x00				;  0x0400 = 1024 clocks per line		
000074 9300 0088                 	sts     OCR1AL, J			; set Counter 1 to cycle from 0 to 1024 and IRQ
000076 c00a                      	rjmp	palOrNtscSet		;
                                 
                                 setNTSC:						; no, set NTSC mode
000077 e006                      	ldi		J, 0x06				; 0x0106 Last vertical line 262
000078 2ea0                      	mov		lastline, J			;
000079 e208                      	ldi		J, 0x28				; first vertical line of active disp
00007a 2eb0                      	mov		line1, J			;
00007b e003                      	ldi		J, 0x03				; 0x03f8 = 1016 clocks per line (63.5us)
00007c 9300 0089                 	sts     OCR1AH, J        
00007e ef08                      	ldi		J, 0xf8				; 0x03f8 = 1016 clocks per line (63.5us)
00007f 9300 0088                 	sts     OCR1AL, J			; set Counter 1 to cycle from 0 to 1016 and IRQ
                                 
                                 palOrNtscSet:
                                 
                                 ;---------------------------------------------------------------------------------------------------------------------------
                                 ;  Initialise TWI if used otherwise setup ack/avail pins
000081 fe35                      	sbrs	configByte,CONFIG_N_TWI	; low = TWI
000082 c003                      	rjmp	twoWireSelected		; Yes, set TWI mode otherwise setup ack/avail pins
                                 
                                 twoWireNotUsed:
000083 e108                      	LDI		J, 0x18				; pins 3,4 of port C are outputs
000084 b907                      	out		DDRC, J	
000085 c008                      	rjmp	twiSet
                                 
                                 twoWireSelected:
000086 e008                      	LDI		J, 0x08				; only pin 3 of port C is an output
000087 b907                      	out		DDRC, J	
                                 
000088 e002                      	ldi		J, 0x02				; Set address to 0x01, format aaaaaaag (a=address, g=general call enable)
000089 9300 00ba                 	sts		TWAR, J				;
00008b e404                      	ldi		J, (1<<TWEA)|(1<<TWEN)	
00008c 9300 00bc                 	sts		TWCR, J				; Enable TWI and allow ACK when slave is addressed.
                                 
                                 twiSet:
                                 
                                 ;---------------------------------------------------------------------------------------------------------------------------
                                 ;  Reset Timer 1 and clear Busy Status Flag
00008e e000                      	ldi     J, 0x00				; clear timer 1
00008f 9300 0085                 	sts     TCNT1H, J			; 
000091 9300 0084                 	sts     TCNT1L, J			; 
                                 
000093 e10c                      	ldi		J, 0x1C				; clear timer 1 IRQ's
000094 bb06                      	out		TIFR1, J			; clear timer 1 IRQ's
                                 
                                 	; Set the default attributes for each line (Screen 0100-08CF (2000 bytes), Attributes for each line 08D0-08E9 (25 bytes) )
000095 ede0                      	ldi		ZL, 0xD0
000096 e0f8                      	ldi		ZH, 0x08			; set to start of attribute SRAM
000097 e1c9                      	ldi		YL, 0x19
000098 e0d0                      	ldi		YH,	0x00			; set Y to 25 (19 HEX)
                                 loop1:
000099 9251                      	st 		Z+, activeLineAtt	; save to attribute SRAM			
00009a 9721                      	sbiw	YL, 0x01			; dec Y
00009b f7e9                      	brne	loop1	
                                 
00009c e07c                      	ldi 	inpt,0x0C			; CLS
00009d d452                      	rcall	processChar
                                 
00009e 9478                      	SEI							; Initialisation complete. Enable IRQ's
                                 		
                                 ;---------------------------------------------------------------------------------------------------------------------------
                                 ; Main Loop 
                                 
                                 main:
00009f 3080                      	cpi		cursorChar, 0x00	; is the cursor turned on?
0000a0 f0c1                      	breq	endOfCursor			; no, skip cursor code
0000a1 3f9f                      	cpi		cursorClk, 0xff		; is clk at 0xFF?
0000a2 f041                      	breq	solidCursor			; yes - solid cursor (no blink)
0000a3 319c                      	cpi		cursorClk, 0x1C		; is it above 28 frames?
0000a4 f462                      	brpl	blinkCursor			; if >25, show Cursor
0000a5 2d77                      	mov		inpt,charUnderCursor
0000a6 940e 05f7                 	call	setYfromRowCol
0000a8 940e 0600                 	call	putCharAtY
0000aa c00e                      	rjmp	endOfCursor			
                                 solidCursor:
0000ab 2f78                      	mov		inpt,cursorChar
0000ac 940e 05f7                 	call	setYfromRowCol
0000ae 940e 0600                 	call	putCharAtY
0000b0 c008                      	rjmp	endOfCursor
                                 blinkCursor:
0000b1 2f78                      	mov		inpt,cursorChar
0000b2 940e 05f7                 	call	setYfromRowCol
0000b4 940e 0600                 	call	putCharAtY
0000b6 3397                      	cpi		cursorClk, 0x37		; is clock >56 
0000b7 f00a                      	brmi	endOfCursor			; no
0000b8 2799                      	clr		cursorClk				; reset timer
                                 endOfCursor:
                                 
0000b9 fe35                      	sbrs	configByte,CONFIG_N_TWI	; low = TWI
0000ba c032                      	rjmp	twiMode			; Yes, set TWI mode
                                 
                                 eightOrFourBit:
                                 	; If the ack and data rdy are the same state then there is no
                                 	; data waiting
0000bb b108                      	in		J,PORTC
0000bc 7100                      	andi	J,0x10
0000bd b116                      	in		K,PINC
0000be 7210                      	andi	K,0x20
0000bf 9516                      	lsr		K
0000c0 2701                      	eor		J,K
0000c1 f2e9                      	breq	Main
                                 
