0000e4 9506                      	lsr		J					; Move to lower 4 bits
0000e5 2a20                      	or		nibbleTemp,J		; Combine it with the first 4 bits
0000e6 2d72                      	mov		inpt,nibbleTemp
                                 
                                 	; Data read, so flip the ack bit
0000e7 b118                      	in		K,PORTC
0000e8 e100                      	ldi		J,0x10
0000e9 2710                      	eor		K,J
